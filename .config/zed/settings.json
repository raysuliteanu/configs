{
  "base_keymap": "JetBrains",
  "vim_mode": true,
  "ui_font_size": 20,
  "buffer_font_size": 20,
  "theme": {
    "mode": "dark",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha"
  },
  "relative_line_numbers": true,
  "inline_blame": {
    "enabled": false
  },
  "inlayHints": {
    "maxLength": null,
    "lifetimeElisionHints": {
      "useParameterNames": true,
      "enable": "skip_trivial"
    },
    "closureReturnTypeHints": {
      "enable": "always"
    }
  },
  "copy_on_select": true
}
