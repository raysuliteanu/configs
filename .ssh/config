Host git.jetbrains.space
  User git
  HostName git.jetbrains.space
  PreferredAuthentications publickey
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_rsa

Host rxs-pi.local
  User ray
  HostName rxs-pi.local
  PreferredAuthentications publickey
  IdentitiesOnly yes
  IdentityFile ~/.ssh/rxs-pi-rsa
  ForwardX11 yes

Host *
  IdentityAgent ~/.1password/agent.sock
  AddKeysToAgent yes

